{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "variable-name": false,
    "one-variable-per-declaration": false,
    "no-inferrable-types": false,
    "max-line-length": false,
  }
}
